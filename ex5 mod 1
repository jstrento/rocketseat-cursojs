<body>
    <script>
    function habilidades(usuarios) {
        for (usuario of usuarios) {
             console.log("A " + usuario.nome + " possui as habilidades: " +  usuario.habilidades.join(", ") );
        }
    }
    
    var usuarios = [
        {
            nome: "julia",
            habilidades: ["programacao", "atuar"]
        },
        {
            nome: "camilla",
            habilidades: ["programacao", "cantar"]
        }
    ];
  habilidades(usuarios);
    </script>

</body>
</html>
